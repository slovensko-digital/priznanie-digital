{
  "name": "with-jest",
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "npm run test:noWatch && next build",
    "start": "next start",
    "test": "jest --watch",
    "test:noWatch": "jest",
    "test:ci": "jest --ci && npm run cy:ci",
    "prettier": "prettier --write  './{lib,components,pages}/**/*.*'",
    "cy:open": "cypress open",
    "cy:run": "cypress run --headless",
    "cy:ci": "start-server-and-test dev http://localhost:3000/ cy:run",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:watch": "esw . -w --color --ext .js,.jsx,.ts,.tsx",
    "eslint-check": "eslint --print-config pages/api/_app.tsx | eslint-config-prettier-check"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "formik": "^2.1.4",
    "isomorphic-unfetch": "^3.0.0",
    "js-file-download": "^0.4.10",
    "lodash.floor": "4.0.4",
    "next": "latest",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "xml-js": "^1.6.11",
    "yup": "^0.28.1"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.1.2",
    "@testing-library/jest-dom": "^5.1.0",
    "@testing-library/react": "^9.4.0",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^25.1.3",
    "@types/lodash.round": "^4.0.6",
    "@types/node": "^13.7.4",
    "@types/yup": "0.26.32",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "cypress": "^4.0.2",
    "cypress-file-upload": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-unicorn": "^16.1.1",
    "eslint-watch": "^6.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "jest-raw-loader": "^1.0.1",
    "libxmljs": "^0.19.7",
    "prettier": "1.19.1",
    "react-test-renderer": "^16.12.0",
    "start-server-and-test": "1.10.7",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5"
  }
}
