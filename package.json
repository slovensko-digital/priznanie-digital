{
  "name": "priznanie-digital",
  "version": "0.1.0",
  "engines": {
    "node": "12.19.0"
  },
  "scripts": {
    "dev": "WITH_DEBUG=true next dev",
    "dev:cy": "start-server-and-test dev http://localhost:3000/ cy:open",
    "build": "next build",
    "start": "node node_modules/next/dist/bin/next start -p $PORT",
    "test:watch": "npm run test -- --watch",
    "test": "jest",
    "ci": "npm run lint && npm run test -- --ci && WITH_DEBUG=true npm run build && npm run cy:ci",
    "prettier": "prettier --write  './{src,__tests__,cypress}/**/*.{ts,tsx,js,css}'",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:dashboard": "cypress run --record --parallel",
    "cy:ci": "PORT=3000 start-server-and-test start http://localhost:3000/ cy:run:dashboard",
    "lint": "eslint . --format pretty --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "check:ts": "tsc --noEmit",
    "git-comit-hash": "echo {\\\"gitCommit\\\":\\\"$(git rev-parse --short HEAD)\\\"} > ./git-commit.json && git add ./git-commit.json"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "decimal.js": "10.2.1",
    "formik": "2.2.6",
    "fuse.js": "5.2.3",
    "hummus": "^1.0.108",
    "iban": "0.0.14",
    "js-file-download": "0.4.12",
    "lodash.clonedeep": "4.5.0",
    "memory-streams": "0.1.3",
    "next": "^9.5.5",
    "node-fetch": "2.6.1",
    "nookies": "2.3.2",
    "react": "16.12.0",
    "react-autosuggest": "9.4.3",
    "react-dom": "16.12.0",
    "rodnecislo": "1.6.0",
    "xml-js": "1.6.11",
    "yup": "0.28.5"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "4.1.5",
    "@types/classnames": "2.2.11",
    "@types/gtag.js": "0.0.3",
    "@types/iban": "0.0.32",
    "@types/jest": "25.2.3",
    "@types/lodash.clonedeep": "4.5.6",
    "@types/node": "13.13.30",
    "@types/react-autosuggest": "9.3.14",
    "@types/xml2js": "0.4.5",
    "@types/yup": "0.29.9",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "babel-eslint": "10.1.0",
    "cypress": "5.4.0",
    "cypress-file-upload": "3.5.3",
    "eslint": "6.8.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-formatter-pretty": "3.0.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-flowtype": "4.7.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-only-warn": "1.0.2",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "2.5.1",
    "eslint-plugin-sonarjs": "0.5.0",
    "eslint-plugin-unicorn": "16.1.1",
    "husky": "4.3.7",
    "identity-obj-proxy": "3.0.0",
    "jest": "25.5.4",
    "jest-raw-loader": "1.0.1",
    "lint-staged": "10.2.11",
    "prettier": "2.2.1",
    "react-test-renderer": "16.12.0",
    "start-server-and-test": "^1.11.7",
    "ts-jest": "25.5.1",
    "ts-loader": "6.2.2",
    "typescript": "3.9.6",
    "webpack": "4.44.2",
    "xml2js": "0.4.23"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run git-comit-hash"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
